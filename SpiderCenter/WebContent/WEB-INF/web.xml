<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 	xmlns="http://java.sun.com/xml/ns/javaee" 	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  
	<servlet>
		<servlet-name>MornitorServlet</servlet-name>
		<servlet-class>com.aizhizu.servlet.MornitorServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MornitorServlet</servlet-name>
		<url-pattern>/mornitor</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ScheduledServlet</servlet-name>
		<servlet-class>com.aizhizu.servlet.ScheduledServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ScheduledServlet</servlet-name>
		<url-pattern>/scheduled</url-pattern>
	</servlet-mapping>
	
	<listener>
	<listener-class>com.aizhizu.servlet.MainServlet</listener-class>
	</listener>	
	
	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  
  
</web-app>
